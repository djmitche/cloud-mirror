    "aws-sdk": "^2.3.7",
    "aws-sdk-promise": "0.0.2",
    "babel-compile": "^2.0.0",
    "babel-preset-node5": "^11.0.2",
    "bluebird": "^3.2.1",
    "commander": "^2.9.0",
    "continuation-local-storage": "^3.1.4",
    "cookie": "^0.2.3",
    "debug": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-config-taskcluster": "^1.0.2",
    "eslint-plugin-taskcluster": "^1.0.2",
    "mocha": "^2.3.3",
