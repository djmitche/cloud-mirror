    "babel-eslint": "^4.1.8",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-node5": "^10.0.0",
    "babel-preset-stage-1": "^6.1.18",
    "eslint": "^1.10.3",
    "mocha": "^2.3.3",
    "sinon": "^1.17.3",
