{
  "name": "cloud-mirror",
  "version": "0.0.1",
  "description": "Proxy files in one cloud storage system to another",
  "main": "index.js",
  "scripts": {
    "compile": "babel-compile -c tc-rules/babel-node5 src:lib test-src:test",
    "prepublish": "npm run compile",
    "pretest": "npm run compile",
    "test": "mocha --require source-map-support/register -t 30s test/*_test.js",
    "lint": "eslint --rulesdir eslint-rules src test-src",
    "debugtest": "mocha debug --require source-map-support/register -t 30s test/*_test.js"
  },
